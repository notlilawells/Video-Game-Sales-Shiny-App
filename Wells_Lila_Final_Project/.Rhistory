)
# Creating the plot
ggplot(data = steph_curry, aes(x = LOC_X, y = LOC_Y)) +
annotation_custom(
grob = court,
xmin = -250, xmax = 250,
ymin = -52, ymax = 418) +
coord_fixed() +
xlim(250, -250) +
ylim(-52, 418) +
# Adding different hexagons
geom_hex(bins = 20, alpha = 0.8, color = "grey") +
# Specifying the plot theme
theme_void() +
# Adding plot title and subtitle
labs(
title = "Stephen Curry",
subtitle = "Shot Chart (2014 - 2015)"
) +
# Formatting the plot title and subtitle
theme(
plot.title = element_text(size = 14, face = "bold"),
plot.subtitle= element_text(size = 12)) +
# Specifying the color scale
scale_fill_gradient(low = "yellow",
high = "red",
na.value = "red",
limits = c(0, 15),
breaks = seq(0, 15, 5),
labels = c("0", "5", "10", "15+"),
name = "Shot \nAttempts")
