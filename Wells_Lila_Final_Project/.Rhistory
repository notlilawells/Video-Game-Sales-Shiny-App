gt(pizza_summary, rowname_col = "name", groupname_col = "type") %>%
row_group_order(groups = c("classic", "chicken")) %>%
tab_header(
title = md("**Pizza Place Annual Pizza Sales**"),
subtitle = "Fiscal Year 2015") %>%
cols_label(
